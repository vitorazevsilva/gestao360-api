{
  "name": "gestao360-api",
  "version": "1.0.0",
  "description": "API for Gest√£o 360",
  "main": "src/server.js",
  "type": "commonjs",
  "repository": "git@github.com:vitorazevsilva/gestao360-api.git",
  "author": {
    "name": " Vitor Silva",
    "email": "vitorsilva10413@gmail.com"
  },
  "scripts": {
    "dev": "nodemon app/server.js",
    "start": "node app/server.js",
    "test": "NODE_ENV=test jest --coverage --runInBand --forceExit",
    "secure-mode": "NODE_ENV=test jest --watchAll --verbose=true",
    "lint": "eslint src/**/* test/**"
  },
  "directories": {
    "test": "test",
    "app": "app"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "nodemonConfig": {
    "restartable": "rs",
    "verbose": true,
    "ignore": [
      "coverage",
      "logs",
      "node_modules",
      ".vscode",
      "test",
      ".eslintrc.js",
      ".gitignore",
      "package.json",
      "yarn.lock"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "get-random-string": "^1.0.9",
    "handlebars": "^4.7.8",
    "knex": "^3.1.0",
    "nodemailer": "^6.9.10",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  }
}