{
  "name": "gestao360-api",
  "version": "1.0.0",
  "description": "API for Gest√£o 360",
  "main": "app/server.js",
  "type": "commonjs",
  "repository": "git@github.com:vitorazevsilva/gestao360-api.git",
  "author": {
    "name": " Vitor Silva",
    "email": "vitorsilva10413@gmail.com"
  },
  "scripts": {
    "dev": "nodemon app/server.js",
    "start": "node app/server.js",
    "test": "NODE_ENV=test jest --coverage --runInBand --forceExit",
    "secure-mode": "NODE_ENV=test jest --watchAll --verbose=true",
    "lint": "eslint app/**/* test/**",
    "db": "docker compose up -d",
    "db:sudo": "sudo docker compose up -d",
    "db:stop:sudo": "docker compose stop",
    "knex_m:make": "knex migrate:make",
    "knex_m:run": "knex migrate:latest",
    "knex_m:back": "knex migrate:rollback",
    "knex_s:make": "knex seed:make",
    "knex_s:run": "knex seed:run"
  },
  "directories": {
    "test": "test",
    "app": "app"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "nodemonConfig": {
    "restartable": "rs",
    "verbose": true,
    "ignore": [
      "coverage",
      "logs",
      "node_modules",
      ".vscode",
      "test",
      ".eslintrc.js",
      ".gitignore",
      "package.json",
      "yarn.lock"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "consign": "^0.1.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-discord-webhook": "^2.0.0"
  }
}
